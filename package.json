{
  "name": "fegs-scoping-tool",
  "version": "2.0.0",
  "private": true,
  "description": "Describe utilization of Final Ecosystem Goods and Services (FEGS).",
  "main": "src/main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "start": "set NODE_ENV=dev && electron ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/USEPA/fegs-dashboard.git"
  },
  "keywords": [
    "FEGS"
  ],
  "contributors": [
    "Kyle Thomas",
    "Seth Jenkins",
    "Jeremy King"
  ],
  "build": {
    "appId": "FEGS-Scoping-Tool",
    "productName": "FEGS Scoping Tool",
    "nsis": {
      "oneClick": true
    },
    "fileAssociations": [
      {
        "ext": [
          "fegs"
        ],
        "name": "FEGS",
        "description": "FEGS file extension",
        "role": "Editor"
      }
    ]
  },
  "devDependencies": {
    "electron": "^10.1.3",
    "electron-builder": "^22.8.1"
  }
}
